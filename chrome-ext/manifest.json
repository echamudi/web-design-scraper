{
  "name": "Web Design Scraper",
  "version": "1.0.0",
  "description": "Extract web design metrics from websites!",
  "manifest_version": 3,
  "permissions": [
    "tabCapture",
    "activeTab",
    "storage",
    "tabs",
    "unlimitedStorage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "preferences.html"
}